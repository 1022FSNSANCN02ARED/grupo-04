<%- include('../partials/head') %>
<body class="body-login">
    <%- include('../partials/header') %>  

<section class="box-form-login">
    <form action= "" method="post" class="formulario">
    
        <h2 class="titulo-login">BIENVENIDOS</h2>
      <% if(locals.errors && errors.name){ %>
       <p><%= errors.name %></p>
       <% }%>
        <div class="input-box">
            <label for="">Ingrese Usuario</label>
            <input type="text" name="usuarioLogin" id="usuarioLogin" class="campo-input" >
        </div>
    
        <div class="input-box">
            <label for="">Ingrese Contraseña</label>
            <input type="password" name="passwordLogin" id="passwordLogin" class="campo-input" >
        </div>
    
        
        <button  class="boton-login" type="submit">Ingresar</button>
    
        <div class="input-box check-login" >
            <label > Recordar </label>
            <div class="check"><input type="checkbox"   checked="checked" name="remember"> </div>
        </div>
    
            <div class="olvido-contraseña"> <a href="#"> ¿Olvidó su contraseña? </a></div>
            
    </form>
</section>
    




<footer>
<%- include('../partials/footer') %>  
</footer>
</body>
</html>